<dataset type="EDDTableFromMultidimNcFiles" datasetID="jklymak_mvp" active="true">
    <reloadEveryNMinutes>10080</reloadEveryNMinutes>
    <updateEveryNMillis>10000</updateEveryNMillis>
    <fileDir>/datasets/uvic/jklymak/mvp/</fileDir>
    <fileNameRegex>survey.*\.nc</fileNameRegex>
    <recursive>true</recursive>
    <pathRegex>.*</pathRegex>
    <metadataFrom>last</metadataFrom>
    <standardizeWhat>0</standardizeWhat>
    <removeMVRows>true</removeMVRows>
    <sortFilesBySourceNames></sortFilesBySourceNames>
    <fileTableInMemory>false</fileTableInMemory>
    <!-- <cacheFromUrl>https://ocean-physics.seos.uvic.ca/~jklymak/PW13DataAnalysis/mvp/ncgrid/</cacheFromUrl> -->
    <accessibleViaFiles>True</accessibleViaFiles>
    <!-- sourceAttributes>
    <att name="_NCProperties">version=1|netcdflibversion=4.4.1|hdf5libversion=1.8.17</att>
</sourceAttributes -->
    <!-- Please specify the actual cdm_data_type (TimeSeries?) and related info below, for example...
    <att name="cdm_timeseries_variables">station_id, longitude, latitude</att>
    <att name="subsetVariables">station_id, longitude, latitude</att>
-->
    <addAttributes>
        <att name="_NCProperties">null</att>
        <att name="Conventions">COARDS, CF-1.6, ACDD-1.3</att>
        <att name="infoUrl">https://ocean-physics.seos.uvic.ca/~jklymak/</att>
        <att name="institution">UVic</att>
        <att name="keywords">analog, bottom, cast, cast_number, cond, data, density, depth, depths, earth, Earth Science &gt; Oceans &gt; Salinity/Density &gt; Salinity, latitude, local, longitude, matlabtime, mtime, number, O2, ocean, oceans, oxygen, pden, practical, pressure, salinity, science, sea, sea_water_practical_salinity, seawater, source, spice, temperature, time, water</att>
        <att name="keywords_vocabulary">GCMD Science Keywords</att>
        <att name="license">[standard]</att>
        <att name="sourceUrl">(local files)</att>
        <att name="standard_name_vocabulary">CF Standard Name Table v70</att>
        <att name="subsetVariables">filename</att>
        <att name="summary">Data from https://ocean-physics.seos.uvic.ca/~jklymak/PW13DataAnalysis/mvp/ncgrid/.</att>
        <att name="title">JKlymak Moving Vessel Profiler</att>
        <att name="cdm_data_type">TrajectoryProfile</att>
        <att name="cdm_trajectory_variables">filename</att>
        <att name="cdm_profile_variables">cast_number,time</att>
    </addAttributes>
    <dataVariable>
        <sourceName>***fileName,(.*),1</sourceName>
        <destinationName>filename</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
    </sourceAttributes -->
        <addAttributes>
            <att name="long_name">File Name</att>
            <att name="cf_role">trajectory_id</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>cast_number</sourceName>
        <destinationName>cast_number</destinationName>
        <dataType>int</dataType>
        <!-- sourceAttributes>
    </sourceAttributes -->
        <addAttributes>
            <att name="_FillValue" type="int">2147483647</att>
            <att name="colorBarMaximum" type="double">100.0</att>
            <att name="colorBarMinimum" type="double">0.0</att>
            <att name="long_name">Cast Number</att>
            <att name="cf_role">profile_id</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>matlabtime</sourceName>
        <destinationName>time</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="long_name">Time</att>
            <att name="standard_name">time</att>
            <att name="units">days since 0000-01-02T00:00:00</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>longitude</sourceName>
        <destinationName>longitude</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="colorBarMaximum" type="double">180.0</att>
            <att name="colorBarMinimum" type="double">-180.0</att>
            <att name="long_name">Longitude</att>
            <att name="standard_name">longitude</att>
            <att name="units">degrees_east</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>latitude</sourceName>
        <destinationName>latitude</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="colorBarMaximum" type="double">90.0</att>
            <att name="colorBarMinimum" type="double">-90.0</att>
            <att name="long_name">Latitude</att>
            <att name="standard_name">latitude</att>
            <att name="units">degrees_north</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>depths</sourceName>
        <destinationName>depth</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="colorBarMaximum" type="double">8000.0</att>
            <att name="colorBarMinimum" type="double">-8000.0</att>
            <att name="colorBarPalette">TopographyDepth</att>
            <att name="long_name">Depth</att>
            <att name="standard_name">depth</att>
            <att name="units">m</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>pressure</sourceName>
        <destinationName>pressure</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
        <att name="coordinates">longitude matlabtime latitude time mtime</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="coordinates">null</att>
            <att name="long_name">Pressure</att>
            <att name="missing_value" type="double">NaN</att>
            <att name="standard_name">sea_water_pressure</att>
            <att name="units">dbar</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>pden</sourceName>
        <destinationName>pden</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
        <att name="coordinates">longitude matlabtime latitude time mtime</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="coordinates">null</att>
            <att name="long_name">Pden</att>
            <att name="missing_value" type="double">NaN</att>
            <att name="standard_name">sea_water_potential_density</att>
            <att name="units">kg/m^3</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>cond</sourceName>
        <destinationName>cond</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
        <att name="coordinates">longitude matlabtime latitude time mtime</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="coordinates">null</att>
            <att name="long_name">Conductivity</att>
            <att name="missing_value" type="double">NaN</att>
            <att name="units">mS/cm</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>temp</sourceName>
        <destinationName>temp</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
        <att name="coordinates">longitude matlabtime latitude time mtime</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="coordinates">null</att>
            <att name="long_name">Temperature</att>
            <att name="missing_value" type="double">NaN</att>
            <att name="standard_name">sea_water_temprature</att>
            <att name="units">degrees_celsius</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>analog</sourceName>
        <destinationName>analog</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
        <att name="coordinates">longitude matlabtime latitude time mtime</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="coordinates">null</att>
            <att name="long_name">Analog</att>
            <att name="missing_value" type="double">NaN</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>density</sourceName>
        <destinationName>density</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
        <att name="coordinates">longitude matlabtime latitude time mtime</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="coordinates">null</att>
            <att name="long_name">Density</att>
            <att name="missing_value" type="double">NaN</att>
            <att name="standard_name">sea_water_density</att>
            <att name="units">kg/m^3</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>salinity</sourceName>
        <destinationName>salinity</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
        <att name="coordinates">longitude matlabtime latitude time mtime</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="colorBarMaximum" type="double">37.0</att>
            <att name="colorBarMinimum" type="double">32.0</att>
            <att name="coordinates">null</att>
            <att name="long_name">Sea Water Practical Salinity</att>
            <att name="missing_value" type="double">NaN</att>
            <att name="standard_name">sea_water_practical_salinity</att>
            <att name="units">PSU</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>bottom</sourceName>
        <destinationName>bottom</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        <att name="_FillValue" type="double">NaN</att>
        <att name="coordinates">longitude matlabtime latitude time mtime</att>
    </sourceAttributes -->
        <addAttributes>
            <att name="coordinates">null</att>
            <att name="long_name">Bottom</att>
            <att name="standard_name">sea_floor_depth_below_sea_surface</att>
            <att name="units">m</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>O2</sourceName>
        <destinationName>O2</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
