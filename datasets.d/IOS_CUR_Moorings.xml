<dataset type="EDDTableFromMultidimNcFiles" datasetID="IOS_CUR_Moorings" active="true">
    <iso19115File>/xml/IOS_CUR_Moorings_iso19115.xml</iso19115File>
    <reloadEveryNMinutes>1440</reloadEveryNMinutes>
    <fileDir>/datasets/IOS_CUR_moorings/</fileDir>
    <fileNameRegex>.*\.nc</fileNameRegex>
    <recursive>true</recursive>
    <pathRegex>.*</pathRegex>
    <metadataFrom>last</metadataFrom>
    <preExtractRegex></preExtractRegex>
    <accessibleViaFiles>true</accessibleViaFiles>
    <postExtractRegex></postExtractRegex>
    <extractRegex></extractRegex>
    <columnNameForExtract></columnNameForExtract>
    <removeMVRows>true</removeMVRows>
    <sortFilesBySourceNames></sortFilesBySourceNames>
    <fileTableInMemory>false</fileTableInMemory>

    <addAttributes>
        <att name="ios_comments"></att>
        <att name="ios_remarks"></att>
        <att name="HEADER"></att>
        <att name="cdm_data_type">TimeSeries</att>
        <att name="cdm_timeseries_variables">profile</att>
        <att name="Conventions">COARDS, CF-1.6, ACDD-1.3</att>
        <att name="creator_name">Institute of Ocean Sciences, 9860 West Saanich Road, Sidney, B.C., Canada</att>
        <att name="infoUrl">???</att>
        <att name="license">[standard]</att>
        <att name="sourceUrl">(local files)</att>
        <att name="subsetVariables">instrument_type, platform, agency, scientist, country, geographic_area</att>
        <att name="standard_name_vocabulary">CF Standard Name Table v79</att>
        <att name="START_TIME">null</att>
        <att name="start_time">null</att>
        <att name="summary">This dataset contains observations made by the Institute of Ocean Sciences of Fisheries and Oceans (DFO) using current meters mounted on moorings. The moorings were deployed by DFO and partner organizations between 1965 to present. Variables present in the dataset include Current speed and direction, Temperature, Depth, and Pressure as a timeseries. For any questions please contact (DFO.PAC.SCI.IOSData-DonneesISO.SCI.PAC.MPO@dfo-mpo.gc.ca).</att>
        <att name="title">IOS Moored Current Meter Data</att>
        <att name="language">eng</att>
        <att name="publisher_name">Hakai Institute</att>
        <att name="keywords_vocabulary">GCMD Science Keywords</att>
        <att name="keywords">depth, pressure, temperature, subSurfaceTemperature, subSurfaceCurrents</att>
        <att name="keywords_fra"></att>
        <att name="title_fra"></att>
        <att name="summary_fra"></att>
        <att name="institution">Institute of Ocean Sciences</att>
        <att name="platform">subsurface mooring</att>
        <att name="platform_vocabulary">http://vocab.nerc.ac.uk/collection/L06/current/</att>
    </addAttributes>

    <dataVariable>
        <sourceName>profile</sourceName>
        <destinationName>profile</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="cf_role">timeseries_id</att>
        </addAttributes>
    </dataVariable>

    <dataVariable>
        <sourceName>filename</sourceName>
        <destinationName>filename</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>country</sourceName>
        <destinationName>country</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>deployment_mission_id</sourceName>
        <destinationName>mission_id</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>scientist</sourceName>
        <destinationName>scientist</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>project</sourceName>
        <destinationName>project</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>agency</sourceName>
        <destinationName>agency</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>platform</sourceName>
        <destinationName>platform</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>instrument_type</sourceName>
        <destinationName>instrument_type</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>instrument_model</sourceName>
        <destinationName>instrument_model</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>instrument_serial_number</sourceName>
        <destinationName>instrument_serial_number</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>geographic_area</sourceName>
        <destinationName>geographic_area</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>event_number</sourceName>
        <destinationName>event_number</destinationName>
        <dataType>String</dataType>
    </dataVariable>

    <dataVariable>
        <sourceName>latitude</sourceName>
        <destinationName>latitude</destinationName>
        <dataType>float</dataType>

        <addAttributes>
            <att name="colorBarMaximum" type="double">90.0</att>
            <att name="colorBarMinimum" type="double">-90.0</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>longitude</sourceName>
        <destinationName>longitude</destinationName>
        <dataType>float</dataType>
        <addAttributes>
            <att name="colorBarMaximum" type="double">180.0</att>
            <att name="colorBarMinimum" type="double">-180.0</att>
            <att name="standard_name">longitude</att>
        </addAttributes>
    </dataVariable>

    <dataVariable>
        <sourceName>time</sourceName>
        <destinationName>time</destinationName>
        <dataType>double</dataType>
        <addAttributes>
            <att name="units">seconds since 1970-01-01T00:00:00Z</att>
        </addAttributes>
    </dataVariable>

    <dataVariable>
        <sourceName>PRESPR01</sourceName>
        <destinationName>PRESPR01</destinationName>
        <dataType>float</dataType>
        <addAttributes>
            <att name="ios_name">pressure</att>
            <att name="long_name">Pressure</att>
            <att name="standard_name">sea_water_pressure</att>
            <att name="units">decibar</att>
            <att name="colorBarMaximum" type="double">8000.0</att>
            <att name="colorBarMinimum" type="double">0.0</att>
            <att name="Maximum">null</att>
            <att name="Minimum">null</att>
        </addAttributes>
    </dataVariable>

    <dataVariable>
        <sourceName>instrument_depth</sourceName>
        <destinationName>instrument_depth</destinationName>
        <dataType>float</dataType>
        <addAttributes>
            <att name="units">m</att>
            <att name="colorBarMaximum">1000.0</att>
            <att name="colorBarMinimum">0.0</att>
            <att name="colorBarPalette">Ocean</att>
            <att name="long_name">Instrument Depth</att>
            <att name="standard_name">instrument_depth</att>
        </addAttributes>
    </dataVariable>

    <dataVariable>
        <sourceName>depth</sourceName>
        <destinationName>depth</destinationName>
        <dataType>float</dataType>
        <addAttributes>
            <att name="ios_name">depth</att>
            <att name="units">m</att>
            <att name="colorBarMaximum" type="double">8000.0</att>
            <att name="colorBarMinimum" type="double">0.0</att>
            <att name="colorBarPalette">TopographyDepth</att>
            <att name="long_name">Depth</att>
            <att name="standard_name">depth</att>
        </addAttributes>
    </dataVariable>


    <dataVariable>
        <sourceName>TEMPPR01</sourceName>
        <destinationName>TEMPPR01</destinationName>
        <dataType>float</dataType>
        <addAttributes>
            <att name="units">deg_C</att>
            <att name="long_name">Sea Water Temperature</att>
            <att name="standard_name" value="sea_water_temperature" />
            <att name="colorBarMaximum" type="double">32.0</att>
            <att name="colorBarMinimum" type="double">0.0</att>
        </addAttributes>
    </dataVariable>

    <dataVariable>
        <sourceName>LCEWEL01</sourceName>
        <destinationName>LCEWEL01</destinationName>
        <dataType>float</dataType>
        <addAttributes>
            <att name="units">m/s</att>
            <att name="long_name">Eastward Sea Water Velocity</att>
            <att name="standard_name">eastward_sea_water_velocity</att>
            <att name="colorBarMaximum" type="double">2.0</att>
            <att name="colorBarMinimum" type="double">-2.0</att>
            <att name="Maximum">null</att>
            <att name="Minimum">null</att>
        </addAttributes>
    </dataVariable>

    <dataVariable>
        <sourceName>LCNSEL01</sourceName>
        <destinationName>LCNSEL01</destinationName>
        <dataType>float</dataType>
        <addAttributes>
            <att name="units">m/s</att>
            <att name="long_name">Northward Sea Water Velocity</att>
            <att name="standard_name">northward_sea_water_velocity</att>
            <att name="colorBarMaximum" type="double">2.0</att>
            <att name="colorBarMinimum" type="double">-2.0</att>
            <att name="Maximum">null</att>
            <att name="Minimum">null</att>
        </addAttributes>
    </dataVariable>

    <dataVariable>
        <sourceName>LRZASP01</sourceName>
        <destinationName>LRZASP01</destinationName>
        <dataType>float</dataType>
        <addAttributes>
            <att name="units">m/s</att>
            <att name="long_name">Northward Sea Water Velocity</att>
            <att name="standard_name">northward_sea_water_velocity</att>
            <att name="colorBarMaximum" type="double">2.0</att>
            <att name="colorBarMinimum" type="double">-2.0</att>
            <att name="Maximum">null</att>
            <att name="Minimum">null</att>
        </addAttributes>
    </dataVariable>

    <dataVariable>
        <sourceName>LCSAEL01</sourceName>
        <destinationName>LCSAEL01</destinationName>
        <dataType>float</dataType>
        <addAttributes>
            <att name="units">m/s</att>
            <att name="long_name">Sea Water Speed</att>
            <att name="standard_name">sea_water_speed</att>
            <att name="colorBarMaximum" type="double">2.0</att>
            <att name="colorBarMinimum" type="double">0.0</att>
            <att name="Maximum">null</att>
            <att name="Minimum">null</att>
        </addAttributes>
    </dataVariable>

    <dataVariable>
        <sourceName>LCDAEL01</sourceName>
        <destinationName>LCDAEL01</destinationName>
        <dataType>float</dataType>
        <addAttributes>
            <att name="units">deg</att>
            <att name="long_name">Sea Water Velocity to Direction</att>
            <att name="standard_name">sea_water_velocity_to_direction</att>
            <att name="colorBarMaximum" type="double">360.0</att>
            <att name="colorBarMinimum" type="double">0.0</att>
            <att name="Maximum">null</att>
            <att name="Minimum">null</att>
        </addAttributes>
    </dataVariable>
</dataset>