<dataset type="EDDTableFromAsciiFiles" datasetID="amundsen_rov_dives" active="true">
    <reloadEveryNMinutes>10080</reloadEveryNMinutes>
    <updateEveryNMillis>10000</updateEveryNMillis>
    <fileDir>/datasets/amundsen_rov_dives/</fileDir>
    <fileNameRegex>.*\.csv</fileNameRegex>
    <recursive>true</recursive>
    <pathRegex>.*</pathRegex>
    <metadataFrom>last</metadataFrom>
    <standardizeWhat>0</standardizeWhat>
    <charset>ISO-8859-1</charset>
    <columnSeparator></columnSeparator>
    <columnNamesRow>1</columnNamesRow>
    <firstDataRow>2</firstDataRow>
    <sortedColumnSourceName>LastModified</sortedColumnSourceName>
    <sortFilesBySourceNames>LastModified</sortFilesBySourceNames>
    <fileTableInMemory>false</fileTableInMemory>
    <!-- sourceAttributes>
    </sourceAttributes -->
    <!-- Please specify the actual cdm_data_type (TimeSeries?) and related info below, for example...
        <att name="cdm_timeseries_variables">station_id, longitude, latitude</att>
        <att name="subsetVariables">station_id, longitude, latitude</att>
    -->
    <addAttributes>
        <att name="cdm_data_type">Other</att>
        <att name="Conventions">COARDS, CF-1.6, ACDD-1.3</att>
        <att name="infoUrl">https://amundsenscience.com/</att>
        <att name="institution">Amundsen Sciences</att>
        <att name="keywords">array, array-data, camera, cameraID, class, comprehensive, data, dive, etag, IsTruncated, key, keys, large, leg, local, max, MaxKeys, modified, name, operated, owner, remotely, rov, sequence, size, source, start, start_time, stewardship, storage, StorageClass, system, time, truncated, type, vessel, year</att>
        <att name="license">[standard]</att>
        <att name="sourceUrl">(local files)</att>
        <att name="standard_name_vocabulary">CF Standard Name Table v70</att>
        <att name="subsetVariables">year, leg, cameraID</att>
        <att name="summary">Amundsen Sciences ROV Dives</att>
        <att name="testOutOfDate">now-1day</att>
        <att name="title">Amundsen ROV Dives videos</att>
    </addAttributes>
    <dataVariable>
        <sourceName>year</sourceName>
        <destinationName>year</destinationName>
        <dataType>short</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="_FillValue" type="short">32767</att>
            <att name="long_name">Year</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>leg</sourceName>
        <destinationName>leg</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="long_name">Leg</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>dive</sourceName>
        <destinationName>dive</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="long_name">Dive</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>cameraID</sourceName>
        <destinationName>cameraID</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="long_name">Camera ID</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>sequence</sourceName>
        <destinationName>sequence</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="long_name">Sequence</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>start_time</sourceName>
        <destinationName>start_time</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="long_name">Start Time</att>
            <att name="standard_name">time</att>
            <att name="time_precision">1970-01-01T00:00:00Z</att>
            <att name="units">yyyyMMdd&#39;T&#39;HHmmss&#39;Z&#39;</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>Key</sourceName>
        <destinationName>video_link</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="long_name">Key</att>
            <att name="fileAccessBaseUrl">https://s3.valeria.science/amundsen-science-public-bucket-test/</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>LastModified</sourceName>
        <destinationName>LastModified</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="long_name">Last Modified</att>
            <att name="source_name">LastModified</att>
            <att name="standard_name">time</att>
            <att name="time_precision">1970-01-01T00:00:00.000Z</att>
            <att name="units">yyyy-MM-dd&#39;T&#39;HH:mm:ss.SSS&#39;Z&#39;</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>ETag</sourceName>
        <destinationName>ETag</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="long_name">ETag</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>Size</sourceName>
        <destinationName>Size</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="long_name">Size</att>
            <att name="units">GB</att>
        </addAttributes>
    </dataVariable>
</dataset>