        <att name="language">eng</att>
        <att name="publisher_name">Hakai Institute</att>
        <att name="keywords_vocabulary">GCMD Science Keywords</att>
        <att name="keywords">nitrate, nitrite, silicate, phosphate, oxygen, subSurfaceSalinity, subSurfaceTemperature,bottle, conductivity, data, density, depth, salinity, nutrients, Niskin, bottle</att>
